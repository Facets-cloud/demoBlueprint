{
  "kind": "helm",
  "flavor": "k8s",
  "disabled": true,
  "metadata": {
    "labels": {
      "deliveryType": "OWN"
    }
  },
  "version": "0.1",
  "spec": {
    "helm": {
      "repository": "https://charts.jenkins.io",
      "chart": "jenkins",
      "namespace": "default",
      "wait": false,
      "version": ""
    },
    "values": {
      "datadog": {
        "service": {
          "annotations": {
            "key": "${blueprint.self.secrets.DEMO_SECRET}"
          }
        }
      }
    },
    "release": {
      "image": "${blueprint.self.artifacts.azure-demo-bp-demo}"
    }
  }
}