{
  "$schema": "https://facets-cloud.github.io/facets-schemas/schemas/tcp_lb/nlb.schema.json",
  "kind": "tcp_lb",
  "flavor": "default",
  "metadata": {
    "name": "test"
  },
  "version": "0.2",
  "spec": {
    "mode": "loadbalancing",
    "instances": 1,
    "ports": {
      "ports1": {
        "port": 80,
        "protocol": "tcp"
      }
    },
    "private": false,
    "selector": {
      "app.kubernetes.io": "app1"
    }
  }
}