{
  "metadata": {
    "name": "retool"
  },
  "disabled": false,
  "flavor": "default",
  "kind": "service",
  "version": "0.1",
  "spec": {
    "type": "application",
    "enable_host_anti_affinity": false,
    "release": {
      "build": {
        "artifactory": "default",
        "name": "demo_be"
      },
      "strategy": {
        "type": "RollingUpdate",
        "max_available": 1,
        "max_unavailable": 0
      }
    },
    "env": {
      "ATLAS_URI": "mongodb://${mongo.anshul.out.interfaces.cluster.username}:${mongo.anshul.out.interfaces.cluster.password}@mongodb-anshul.default.svc.cluster.local:27017",
      "TITLE": "${blueprint.self.variables.TITLE}"
    },
    "runtime": {
      "health_checks": {
        "liveness_url": "/api/employees",
        "readiness_url": "/api/employees",
        "port": "5200",
        "start_up_time": 21,
        "timeout": 100,
        "period": 10
      },
      "ports": {
        "http": {
          "port": "5200",
          "protocol": "tcp"
        }
      },
      "size": {
        "cpu": "100m",
        "memory": "100Mi"
      }
    }
  }
}