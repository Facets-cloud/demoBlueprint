{
  "kind": "service",
  "flavor": "k8s",
  "version": "0.1",
  "metadata": {
    "name": "ui",
    "labels": {
      "deliveryType": "OWN"
    }
  },
  "disabled": true,
  "spec": {
    "type": "application",
    "enable_host_anti_affinity": false,
    "release": {
      "build": {
        "artifactory": "default",
        "name": "demo_ui"
      },
      "strategy": {
        "type": "RollingUpdate",
        "max_available": 1,
        "max_unavailable": 0
      },
      "image": "${blueprint.self.artifacts.azure-demo-bp-ui}"
    },
    "runtime": {
      "size": {
        "cpu": "101m",
        "memory": "100Mi"
      },
      "health_checks": {
        "liveness_url": "main",
        "start_up_time": 15,
        "period": 15,
        "readiness_url": "main",
        "port": 8080,
        "timeout": 3
      },
      "ports": {
        "main": {
          "port": "8080",
          "protocol": "tcp"
        }
      }
    },
    "restart_policy": "OnFailure"
  }
}